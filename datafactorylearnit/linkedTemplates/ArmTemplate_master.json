{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "datafactorylearnit"
		},
		"ls_adls_ecdc_data_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_adls_ecdc_data'"
		},
		"ls_blob_config_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_blob_config'"
		},
		"ls_adls_ecdc_data_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://adlsstoragelearnit.dfs.core.windows.net"
		},
		"ls_http_ecdc_data_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "@{linkedService().baseURL}"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "datafactorylearnit_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_adls_ecdc_data_accountKey": {
						"value": "[parameters('ls_adls_ecdc_data_accountKey')]"
					},
					"ls_blob_config_connectionString": {
						"value": "[parameters('ls_blob_config_connectionString')]"
					},
					"ls_adls_ecdc_data_properties_typeProperties_url": {
						"value": "[parameters('ls_adls_ecdc_data_properties_typeProperties_url')]"
					},
					"ls_http_ecdc_data_properties_typeProperties_url": {
						"value": "[parameters('ls_http_ecdc_data_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}